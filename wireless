
config wifi-device 'radio0'
	option type 'mac80211'
	option channel '11'
	option hwmode '11g'
	option path 'd0070000.sdio/mmc_host/sdio/sdio:0001/sdio:0001:1'
	option disabled '0'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'Amlogic'
	option encryption 'psk2+ccmp'
	option key 'internet'

